#include <iostream>
#include <string>
#include "ring.h"

using namespace std;

int main() {




	//ring::iterator it;

	ring<int>::iterator it;

	it.print();


	ring<string> textring(3);


	for(int i=0; i<textring.size(); i++){
		cout << textring.get(i) << endl;

	}



	return 0;
}
